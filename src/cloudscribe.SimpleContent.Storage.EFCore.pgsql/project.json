    "Npgsql.EntityFrameworkCore.PostgreSQL.Design": {
      "version": "1.1.*",
      "type": "build"
    }
  },

  "tools": {
    "Microsoft.EntityFrameworkCore.Tools.DotNet": "1.1.0-preview4-final"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [ "dotnet5.6", "portable-net451+win8" ],
      "buildOptions": {
        "emitEntryPoint": true
