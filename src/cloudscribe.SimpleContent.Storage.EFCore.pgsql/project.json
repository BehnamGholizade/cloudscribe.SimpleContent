{
  "version": "1.1.1-rc20170305",
  "description": "PostgreSql Entity Framework Core implementation of cloudscribe SimpleContent commands and queries",
  "authors": [ "Joe Audette" ],
  "buildOptions": {
    "emitEntryPoint": true,
    "debugType": "portable"
  },

  "packOptions": {
    "tags": [ "cloudscribe", "commands", "queries", "ef" ],
    "owners": [ "Source Tree Solutions" ],
    "projectUrl": "https://github.com/joeaudette/cloudscribe.SimpleContent",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
    "copyright": "Source Tree Solutions",
    "iconUrl": "https://raw.githubusercontent.com/joeaudette/cloudscribe/master/cloudscribe-icon-32.png",
    "requireLicenseAcceptance": false,
    "releaseNotes": ""
  },


  "dependencies": {
    "NETStandard.Library": "1.6.*",
    "Microsoft.Extensions.Configuration.Json": "1.1.*",
    "Microsoft.Extensions.Configuration": "1.1.*",
    "Microsoft.EntityFrameworkCore": "1.1.*",
    "Microsoft.EntityFrameworkCore.Design": "1.1.*",
    "cloudscribe.SimpleContent.Models": "1.1.1-*",
    "cloudscribe.SimpleContent.Storage.EFCore.Common": "1.1.1-*",
